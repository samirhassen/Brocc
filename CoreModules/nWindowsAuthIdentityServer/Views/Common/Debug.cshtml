@model System.Security.Principal.WindowsPrincipal
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Debug Active Directory Setup</title>
</head>
<body>
    <div>
        <h1>Debug Active Directory Setup</h1>

        @if(Model == null)
        {
            <p>User is not a WindowsPrincipal</p>
        } else if (Model.Identity == null)
        {
            <p>User has no identity</p>
        }
        else
        {
            <h2>Username</h2>
            @Model.Identity.Name
            <h2>Claims</h2>
            <table border="1">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Value</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var claim in Model.Claims)
                    {
                        <tr>
                            <td>@claim.Type</td>
                            <td>@claim.Value</td>
                        </tr>
                    }
                </tbody>
            </table>
        }
    </div>
</body>
</html>