@using nTest.Controllers;
@model ApiDocumentationController.IndexModel
@{
    ViewBag.Title = "nTest - Api Documentation";
}

@section Scripts {

}
<div class="pt-1 pb-2">
    <div class="pull-left"><a class="n-back" href="@(new Uri(NEnv.ServiceRegistry.External["nTest"]).ToString())"><span class="glyphicon glyphicon-arrow-left"></span></a></div>
    <h1 class="adjusted" ng-class="">Api documentation</h1>
</div>
<div class="col-sm-12">
    <table class="table">
        <tbody>
            @foreach(var x in Model.Services)
            {
            <tr>
                <td>
                    <a href="@x.DocumentationUrl">@x.Name</a>                    
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>