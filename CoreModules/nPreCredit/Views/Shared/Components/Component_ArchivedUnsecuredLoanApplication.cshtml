@{
    Layout = null;
}

<script type="text/ng-template" id="archived-unsecured-loan-application.html">
    <div>
        <div class="pt-1 pb-2" ng-if="$ctrl.m.applicationInfo">
            <div class="pull-left"><a class="n-back" ng-click="$ctrl.onBack($event)" href="#"><span class="glyphicon glyphicon-arrow-left"></span></a></div>
            <h1 class="adjusted">Application {{$ctrl.m.applicationInfo.ApplicationNr}}, <span> (archived)</span> <span style="font-size:14px;">{{$ctrl.m.applicationInfo.ProviderName}} {{$ctrl.m.applicationInfo.ApplicationDate | date:'yyyy-MM-dd HH:mm'}} </span></h1>
        </div>

        <div class="row pt-1">
            <div class="col-xs-8">
                <div class="block">
                    <div class="row">
                        <div class="col-xs-12"><h2 class="custom-header">Customer </h2></div>
                    </div>
                    <div style="padding-bottom: 70px; border-top:1px solid #000000; margin-top: 3px;" class="pt-2">
                        <div class="row">
                            <div class="col-xs-6" ng-repeat="a in $ctrl.m.applicants">
                                <application-customerinfo initial-data="a" ></application-customerinfo>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>