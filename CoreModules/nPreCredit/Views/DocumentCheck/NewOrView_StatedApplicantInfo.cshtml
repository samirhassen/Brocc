@{
    var applicantNr = (ViewData.Eval("applicantNr") ?? "").ToString();
}
<h3 class="text-center">Applicant @applicantNr</h3>
<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-xs-6">Stated income per month</label>
        <div class="form-control-static col-xs-6">{{c.formatIncomeForDisplay(c.initialData.applicant@(applicantNr).statedIncome)}}</div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-6">Stated employment</label>
        <div class="form-control-static col-xs-6">{{c.initialData.applicant@(applicantNr).employment | translate}}</div>
    </div>
    <div class="form-group">
        <label class="control-label col-xs-6">Stated employer</label>
        <div class="form-control-static col-xs-6">{{c.initialData.applicant@(applicantNr).employer}}</div>
    </div>
</div>